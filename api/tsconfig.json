{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "ES6",
    "outDir": "dist",
    "baseUrl": ".",
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "downlevelIteration": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "module": "CommonJS",
    "moduleResolution": "node"
  }
}