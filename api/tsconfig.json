{
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "node16",
    "target": "ES2022",
    "esModuleInterop": true,
    "outDir": "dist",
    "baseUrl": ".",
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16",
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true
  }
}